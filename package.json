{
  "name": "inviso-new",
  "version": "1.0.0",
  "description": "A cross-platform tool for designing interactive virtual soundscapes. <br/> Accessible online at: <a link href=\"http://inviso.cc\">inviso.cc</a>",
  "author": "CreativeCodingLab",
  "main": "app.js",
  "scripts": {
    "dev": "run-p dev:sass webpack-server webpack-watch",
    "build": "run-s clean build:dir build:js build:sass",
    "clean": "rimraf build",
    "deploy": "gh-pages -d build/public",
    "lint": "eslint src/js/",
    "webpack-server": "set NODE_ENV=0&& webpack-dev-server --progress --colors --hot --inline --open",
    "webpack-watch": "set NODE_ENV=0&& webpack --progress --colors --watch --cache",
    "dev:sass": "node-sass -w -r src/css/ -o src/public/assets/css/",
    "dev:js": "set NODE_ENV=0&& webpack",
    "build:dir": "copyfiles -u 1 \"src/public/**/*\" build/",
    "build:sass": "node-sass --output-style compressed src/css/ -o build/public/assets/css/",
    "build:js": "set NODE_ENV=1&& webpack"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.20.5",
    "ambisonics": "^0.4.0",
    "es6-promise": "^4.2.8",
    "firebase": "^7.24.0",
    "regenerator-runtime": "^0.13.11",
    "sass-loader": "^6.0.7",
    "three": "^0.142.0",
    "three-obj-loader": "^1.1.3",
    "tween.js": "^16.6.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copyfiles": "^1.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.31.0",
    "gh-pages": "^0.12.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^3.0.0",
    "rimraf": "^2.7.1",
    "webpack": "^2.7.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^1.16.5"
  },
  "engines": {
    "node": ">=8.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anilcamci/inviso-new.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anilcamci/inviso-new/issues"
  },
  "homepage": "https://github.com/anilcamci/inviso-new#readme"
}
